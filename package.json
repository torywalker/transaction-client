    "lint": "eslint \"src/**/*.{js,ts}\" --quiet --fix",
    "test": "NODE_ENV=test jest"
  },
  "main": "./lib/index.js",
  "types": "./lib/main.d.ts",
  "repository": "git@github.com:torywalker/transaction-client.git",
  "author": "Tory <toryjwalker@gmail.com>",
  "license": "GNU 3",
  "dependencies": {
    "bunyan": "^1.8.12",
    "bunyan-format": "^0.2.1",
    "eslint": "^6.5.1"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/bunyan-format": "^0.2.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.11",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "tsc": "^1.20150623.0",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.6.3"
  }
